{
  "name": "react-base",
  "version": "1.0.0",
  "description": "react base project",
  "main": "index.js",
  "scripts": {
    "dev": "vite serve",
    "prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skipping pnpm install",
    "build": "vite build",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --config .prettierrc --write '{*.js,src/**/*.{js,jsx,ts,tsx}}'",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --no-error-on-unmatched-pattern --fix",
    "codegen": "graphql-codegen --config codegen.yml",
    "prepare": "husky install"
  },
  "keywords": [
    "reactjs",
    "base",
    "project"
  ],
  "author": "Ortiz Victor <martinortizpalacio@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "apollo-boost": "^0.4.9",
    "react": "^18.2.0",
    "react-apollo": "^3.1.5",
    "react-apollo-hooks": "^0.5.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.6.9",
    "@apollo/react-hoc": "^4.0.0",
    "@apollo/react-hooks": "^4.0.0",
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@material-ui/icons": "^4.11.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4",
    "vite": "^3.0.8",
    "vite-plugin-html": "^3.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,tsx}": [
      "pnpm format",
      "pnpm lint"
    ]
  }
}