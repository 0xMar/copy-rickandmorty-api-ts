{
  "name": "react-base",
  "version": "1.0.0",
  "description": "react base project",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack-dev-server --config ./webpack.dev.config.js --mode development",
    "build:dll": "webpack --config ./webpack.dll.config.js --mode production",
    "build": "webpack --config ./webpack.config.js --mode production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write '{*.js,src/**/*.{js,jsx,ts,tsx}}'",
    "lint": "eslint src/ --no-error-on-unmatched-pattern --fix",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "keywords": [
    "reactjs",
    "base",
    "project"
  ],
  "author": "Ortiz Victor <martinortizpalacio@gmail.com>",
  "license": "MIT",
  "dependencies": {
